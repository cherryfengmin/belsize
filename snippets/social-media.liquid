<ul class="no-bullets social-icons">
              {% if settings.social_facebook_link != blank %}
                <li>
                  <a target="new" href="{{ settings.social_facebook_link | escape }}" title="{{ 'layout.footer.social_platform' | t: name: shop.name, platform: 'Facebook' }}">
                    <span class="icon icon-facebook" aria-hidden="true"></span>
                    
                  </a>
                </li>
